public class palindrome {
    public static void main(String[] args) {
        int n = 121;// target number
//cheak if it is a palindrome

if(n<0){
    System.out.println("it cant be palindrome");
}
int x=n;
int revNum=0;

while(n>0){
    int d =n%10;
    revNum = revNum*10+d;
    n =n/10;
}
if(revNum==x){
    System.out.println("its is a palindrome");
}
else{
    System.out.println("its is not palindrome");
}
    }   
}
